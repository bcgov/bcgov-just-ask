apiVersion: v1
kind: Service
metadata:
  labels:
    app: "{{ web.name }}"
    group: "{{ app_name }}"
  name: {{ web.name }}
spec:
  ports:
  - name: {{ web.port }}-{{ web.port }}
    port: {{ web.port }}
    protocol: TCP
    targetPort: {{ web.port }}
  selector:
    app: "{{ web.name }}"
  type: ClusterIP
status: {}